{
  "name": "ai-workflow-utils",
  "version": "1.0.5",
  "description": "A comprehensive automation platform that streamlines software development workflows by integrating AI-powered content generation with popular development tools like Jira, Bitbucket, and email systems.",
  "main": "dist/server.js",
  "bin": {
    "ai-workflow-utils": "bin/cli.js",
    "ai-workflow-setup": "bin/setup.js"
  },
  "files": [
    "dist/",
    "ui/dist/",
    "ui/.env.example",
    "bin/",
    "README.md",
    "LICENSE",
    ".env.example"
  ],
  "keywords": [
    "ai",
    "workflow",
    "automation",
    "jira",
    "bitbucket",
    "email",
    "build-management",
    "development-tools",
    "openai",
    "ollama",
    "react",
    "express",
    "nodejs"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anuragarwalkar/ai-workflow-utils.git"
  },
  "bugs": {
    "url": "https://github.com/anuragarwalkar/ai-workflow-utils/issues"
  },
  "homepage": "https://github.com/anuragarwalkar/ai-workflow-utils#readme",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node dist/server.js",
    "start-server": "node dist/server.js",
    "start-server-dev": "nodemon dist/server.js",
    "start-ui": "vite",
    "build-ui": "vite build",
    "build-server": "webpack --config webpack.config.js",
    "start-dev": "concurrently --names \"ui, watch, server\" \"npm run start-ui\" \"npm run build-server:watch\" \"npm run start-server-dev\"",
    "serve": "npm run build && npm run start-server",
    "build-server:watch": "webpack --config webpack.config.js --watch",
    "preview": "vite preview",
    "build": "concurrently \"npm run build-ui\" \"npm run build-server\"",
    "prepublishOnly": "npm run build"
  },
  "author": {
    "name": "Anurag Arwalkar",
    "email": "anurag.arwalkar@gmail.com"
  },
  "license": "MIT",
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@eslint/js": "^9.30.1",
    "@fontsource/roboto": "^5.2.6",
    "@mui/icons-material": "^7.2.0",
    "@mui/material": "^7.2.0",
    "@reduxjs/toolkit": "^2.8.2",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "axios": "^1.8.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "express": "^5.1.0",
    "fluent-ffmpeg": "^2.1.3",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "multer": "^1.4.5-lts.2",
    "nodemailer": "^7.0.5",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-redux": "^9.2.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "vite": "^7.0.4",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "babel-loader": "^9.2.1",
    "concurrently": "^9.2.0",
    "nodemon": "^3.1.10",
    "webpack": "^5.100.1",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  }
}
